{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "aagFactory"
		},
		"AAG Data Lake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://aagstore.dfs.core.windows.net"
		},
		"nyiso_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://mis.nyiso.com/public/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "aagFactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AAG Data Lake_properties_typeProperties_url": {
						"value": "[parameters('AAG Data Lake_properties_typeProperties_url')]"
					},
					"nyiso_properties_typeProperties_url": {
						"value": "[parameters('nyiso_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}